        <texte>
          <italique>Suspension et reprise de la séance</italique>
        </texte>
        <paragraphe valeur_ptsodj="3" ordinal_prise="2" id_preparation="1155237" ordre_absolu_seance="2" id_acteur="PA606171" id_mandat="PM722632" id_nomination_oe="-1" id_nomination_op="PM758870" code_grammaire="SUSP_SEANCE_2_1" code_style="NORMAL" code_parole="" sommaire="0" id_syceron="1432867" valeur="" roledebat="president">
