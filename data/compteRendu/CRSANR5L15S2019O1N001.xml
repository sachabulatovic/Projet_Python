        <paragraphe valeur_ptsodj="3" ordinal_prise="2" id_preparation="1155238" ordre_absolu_seance="3" code_grammaire="SUSP_SEANCE_2_2" code_style="Info Italiques" code_parole="" sommaire="0" id_syceron="1432868" valeur="">
